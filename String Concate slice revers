# String concatination

var1="Harry"
var2="Potter"
var3=var1+var2
print(var3)

#String slice

s='Tonystark'
print(s[3:6])

#String reverse

str="Hello everyone"
print("The original string is:",str)
rev=""
count=len(str)
while count>0:
    rev+=str[count-1]
    count=count-1
print("The reversed string using a while loop is:",rev)





